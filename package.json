{
  "name": "@copper/plugin-iot-rules",
  "version": "1.0.0",
  "description": "Architect (arc.codes) serverless framework plugin that defines IoT Topic Rules and associated Lambdas triggered by the Rules",
  "main": "index.js",
  "scripts": {
    "lint": "eslint",
    "test:unit": "jasmine --config=test/jasmine.json",
    "test": "npm run lint && npm run test:unit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/copperinc/plugin-iot-rules.git"
  },
  "keywords": [
    "serverless",
    "arc",
    "cloudformation"
  ],
  "author": "Copper Inc.",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/copperinc/plugin-iot-rules/issues"
  },
  "homepage": "https://github.com/copperinc/plugin-iot-rules#readme",
  "devDependencies": {
    "@architect/eslint-config": "^1.0.0",
    "eslint": "^7.19.0",
    "jasmine": "^3.6.4"
  },
  "dependencies": {
    "@architect/package": "^6.2.0-RC.0",
    "@architect/sandbox": "^3.4.0-RC.0",
    "enquirer": "^2.3.6"
  },
  "eslintConfig": {
    "extends": "@architect/eslint-config",
    "rules": {
      "indent": [
        "error",
        4
      ],
      "semi": [
        "error",
        "always"
      ]
    }
  },
  "publishConfig": {
    "access": "public"
  }
}
